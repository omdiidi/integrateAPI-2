{
    "rewrites": [
        {
            "source": "/recordstoredemo/:path*",
            "destination": "/recordstoredemo/index.html",
            "has": [
                {
                    "type": "host",
                    "value": "integrateapi.ai"
                }
            ]
        },
        {
            "source": "/recordstoredemo/:path*",
            "destination": "/recordstoredemo/index.html",
            "has": [
                {
                    "type": "host",
                    "value": "www.integrateapi.ai"
                }
            ]
        },
        {
            "source": "/api/:path*",
            "destination": "/api/:path*"
        },
        {
            "source": "/ai4u-embed/:path*",
            "destination": "/ai4u-embed/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html",
            "has": [
                {
                    "type": "host",
                    "value": "integrateapi.ai"
                }
            ]
        },
        {
            "source": "/(.*)",
            "destination": "/index.html",
            "has": [
                {
                    "type": "host",
                    "value": "www.integrateapi.ai"
                }
            ]
        },
        {
            "source": "/(.*)",
            "destination": "/ai4u-embed/index.html",
            "has": [
                {
                    "type": "host",
                    "value": "ai4u.integrateapi.ai"
                }
            ]
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                }
            ]
        }
    ]
}